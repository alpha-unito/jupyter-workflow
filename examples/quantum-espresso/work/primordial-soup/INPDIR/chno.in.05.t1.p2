&CONTROL
  title = ' H20+CH4+NH3 ',
  calculation = 'vc-cp',
  restart_mode = 'restart', ! 'restart',
  ndr = 50,
  ndw = 50,
  nstep  = 1000,
  iprint = 10, 
  isave  = 100,
  dt    = 5.0d0,
  etot_conv_thr = 1.d-8,
  prefix = 'chno'
  outdir = './OUTDIR.t1.p2'
  pseudo_dir = './INPDIR'
/

&SYSTEM
  ibrav = 14, 
  celldm(1) = 21.00, 
  celldm(2) = 1.0, 
  celldm(3) = 1.0, 
  celldm(4) = 0.0, 
  celldm(5) = 0.0, 
  celldm(6) = 0.0, 
  nat = 243,
  ntyp = 4,
  ecutwfc = 80.0,
  ecfixed = 70.0,
  qcutz = 70.0,
  q2sigma = 5.0,
/

&ELECTRONS
  emass = 400.d0,
  emass_cutoff = 2.5d0,
  orthogonalization = 'ortho',
  ortho_eps = 2.d-7,
  ortho_max = 100,
  electron_dynamics = 'verlet',
  electron_velocities = 'zero'
/


&IONS
  ion_dynamics = 'verlet',
  ion_radius(1) = 0.8d0,
  ion_radius(2) = 0.8d0,
  ion_radius(3) = 0.8d0,
  ion_radius(4) = 0.5d0,
  ion_temperature = 'nose',
  tempw = 500.
  fnosep = 500.
/



&CELL
  cell_dynamics = 'pr',
  cell_velocities = 'zero',
  press = 20.0d0,
  wmass = 100000.0d0
/


